{
  "name": "it-community-server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run build && DEBUG=* node ./dist/bin/www.js",
    "build": "tsc && npm run cp-static-files",
    "clean": "rm -rf ./dist/",
    "dev": "export PORT=3456 && nodemon --inspect --watch 'src/**/*' -e ts,tsx --exec 'node -r ts-node/register' ./src/bin/www.ts",
    "cp-static-files": "cp -r ./src/public ./dist && cp -r ./src/views ./dist"
  },
  "author": "",
  "license": "",
  "dependencies": {
    "debug": "^4.1.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-json": "^2.0.2",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.0",
    "pug": "*"
  },
  "devDependencies": {
    "@types/debug": "^4.1.3",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.2.2",
    "@types/koa-json": "^2.0.18",
    "@types/koa-router": "^7.0.40",
    "@types/koa-static": "^4.0.1",
    "@types/koa-views": "^2.0.3",
    "@types/node": "^11.13.0",
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.1",
    "@types/pug": "*"
  }
}
